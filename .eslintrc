{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4],
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "max-len": 0,
        "comma-dangle": ["error", "always-multiline"],
        "class-methods-use-this": 0,
        "no-underscore-dangle": 0,
        "no-use-before-define": [2, "nofunc"]
    }
}
